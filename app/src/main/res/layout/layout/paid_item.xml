<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android">
    <data>

    </data>
    <LinearLayout
        android:id="@+id/backLL"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
 
        android:padding="10dp"
        android:layout_height="wrap_content"
       >

        <TextView
            android:layout_width="@dimen/dp_80"
            android:id="@+id/snoTv"
            android:paddingStart="@dimen/dp_10"
            android:paddingEnd="@dimen/dp_10"
            android:text="1"
            android:gravity="center|start"
            android:textColor="@android:color/black"
            android:layout_height="wrap_content"/>

        <TextView
            android:layout_width="@dimen/dp_100"
            android:id="@+id/fromDateTv"
            android:paddingStart="@dimen/dp_10"
            android:paddingEnd="@dimen/dp_10"
            android:text="2019-09-21"
            android:gravity="center"
            android:textColor="@android:color/black"
            android:layout_height="match_parent"/>

        <TextView
            android:layout_width="@dimen/dp_100"
            android:id="@+id/toDateTv"
            android:paddingStart="@dimen/dp_10"
            android:paddingEnd="@dimen/dp_10"
            android:gravity="center"
            android:text="2019-09-27"
            android:textColor="@android:color/black"
            android:layout_height="match_parent"/>

        <TextView
            android:layout_width="150dp"
            android:id="@+id/totalOrderTv"
            android:gravity="center"
            android:paddingStart="@dimen/dp_10"
            android:paddingEnd="@dimen/dp_10"
            android:text=""
            android:textColor="@android:color/black"
            android:layout_height="match_parent"/>

        <TextView
            android:layout_width="150dp"
            android:id="@+id/totalDistanceTv"
            android:paddingStart="@dimen/dp_10"
            android:paddingEnd="@dimen/dp_10"
            android:gravity="center"
            android:text=""
            android:textColor="@android:color/black"
            android:layout_height="match_parent"/>

        <TextView
            android:layout_width="150dp"
            android:id="@+id/orderAmtTv"
            android:gravity="center"
            android:paddingStart="@dimen/dp_10"
            android:paddingEnd="@dimen/dp_10"
            android:text=""
            android:textColor="@android:color/black"
            android:layout_height="match_parent"/>

        <TextView
            android:layout_width="150dp"
            android:id="@+id/deductionTv"
            android:gravity="center"
            android:paddingStart="@dimen/dp_10"
            android:paddingEnd="@dimen/dp_10"
            android:text="0"
            android:textColor="@android:color/black"
            android:layout_height="match_parent"/>
        <TextView
            android:layout_width="200dp"
            android:id="@+id/commissionTv"
            android:gravity="center"
            android:paddingStart="@dimen/dp_10"
            android:paddingEnd="@dimen/dp_10"
            android:text="0"
            android:textColor="@android:color/black"
            android:layout_height="match_parent"/>
        <TextView
            android:visibility="gone"
            android:layout_width="@dimen/dp_100"
            android:id="@+id/feeTv"
            android:gravity="center"
            android:textStyle="bold"
            android:paddingStart="@dimen/dp_10"
            android:paddingEnd="@dimen/dp_10"
            android:text=""
            android:textAllCaps="true"
            android:textColor="@android:color/black"
            android:layout_height="match_parent"/>
        <TextView
            android:layout_width="150dp"
            android:id="@+id/payableAmtTv"
            android:gravity="center"
            android:paddingStart="@dimen/dp_10"
            android:paddingEnd="@dimen/dp_10"
            android:text="0"
            android:textColor="@android:color/black"
            android:layout_height="match_parent"/>
        <TextView
            android:layout_width="150dp"
            android:id="@+id/actionTv"
            android:gravity="center"
            android:paddingStart="@dimen/dp_10"
            android:paddingEnd="@dimen/dp_10"
            android:text="ViewDetails"
            android:textColor="@color/colorPrimaryDark"
            android:layout_height="match_parent"/>
    </LinearLayout>
</layout>

